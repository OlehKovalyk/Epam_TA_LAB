log4j.debug = false

log4j.rootLogger=WARN, book

log4j.appender.book=org.apache.log4j.FileAppender
log4j.appender.book.file=src\\java\\log\\book.log
log4j.appender.book.append=false
log4j.appender.book.layout=org.apache.log4j.EnhancedPatternLayout
log4j.appender.book.layout.conversionPattern=%d{ISO8601} [%-5p][%-16.16t][%30c] - %m%n